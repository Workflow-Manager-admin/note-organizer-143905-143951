# Example environment file for notes_backend

# Database connection string (SQLAlchemy format)
# Example for SQLite (local): sqlite:///./notes.db
# Example for Postgres: postgresql+psycopg2://user:password@host:port/database
NOTES_DATABASE_URL=sqlite:///./notes.db

# Security settings
JWT_SECRET_KEY=replace_this_with_a_secret_key
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# For production deployment, configure allowed origins (comma-separated)
CORS_ALLOW_ORIGINS=*
